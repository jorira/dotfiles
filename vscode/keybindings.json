// 既定値を上書きするには、このファイル内にキー バインドを挿入します
[
    {
        "key": "ctrl+l",
        "command": "cursorRight",
        "when": "editorTextFocus && !editorReadOnly"
    },
    {
        "key": "ctrl+h",
        "command": "cursorLeft",
        "when": "editorTextFocus && !editorReadOnly"
    },
    {
        "key": "ctrl+j",
        "command": "cursorUp",
        "when": "editorTextFocus && !editorReadOnly"
    },
    {
        "key": "ctrl+k",
        "command": "cursorDown",
        "when": "editorTextFocus && !editorReadOnly"
    },
]